<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:DockLayoutPanel unit="EM">
        <g:west size="5">
            <!-- link should be defined in Anchor tag as an attribute via href="/myurl" -->
            <g:Anchor ui:field="logoURL">Aurora</g:Anchor>
        </g:west>

        <g:east size="5">
            <!-- link should be defined in Anchor tag as an attribute via href="/myurl" -->
            <g:Anchor ui:field="tutorialURL">TUTORIAL</g:Anchor>
        </g:east>
    </g:DockLayoutPanel>

</ui:UiBinder>