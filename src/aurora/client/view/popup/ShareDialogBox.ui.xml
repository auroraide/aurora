<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <g:VerticalPanel>
        <g:HorizontalPanel>
            <g:TextArea ui:field="shareText"/>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="copyToClipboardButton" debugId="copyToClipboardButton"><ui:msg description="Cope To Clipboard">Copy to Clipboard</ui:msg></g:Button>
            <g:Button ui:field="closeButton" debugId="closeButton"><ui:msg description="Share Cancel">CANCEL</ui:msg></g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>