<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <g:VerticalPanel>
        <g:HorizontalPanel>
            <g:Button ui:field="openTabButton">To new Tab</g:Button>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="latexExportButton">Export to LaTeX</g:Button>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="toPastebinButton">To Pastebin</g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>
