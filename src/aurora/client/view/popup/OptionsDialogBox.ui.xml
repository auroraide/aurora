<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <g:VerticalPanel>
        <g:HorizontalPanel>
            <g:Button ui:field="openTabButton"><ui:msg description="to NewTab">To new Tab</ui:msg></g:Button>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="latexExportButton"><ui:msg description="Export to Latex">Export to LaTeX</ui:msg></g:Button>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="toPastebinButton"><ui:msg description="to Pastebin">To Pastebin</ui:msg></g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>
