<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <g:FlowPanel>
            <g:RadioButton ui:field="callByValue" debugId="callByValue" name="strategyButton" value="false">
                <ui:msg description="Strategy callByValue" key="callByValue">Call by<br />VALUE</ui:msg>
            </g:RadioButton>

            <g:RadioButton ui:field="callByName" debugId="callByName" name="strategyButton" value="false">
                <ui:msg description="Strategy callByName" key="callByName">Call by <br />NAME</ui:msg>
            </g:RadioButton>

        <g:RadioButton ui:field="normalOrder" debugId="normalOrder" name="strategyButton" value="true">
            <ui:msg description="Strategy normalOrder" key="normalOrder">Normal Order</ui:msg>
        </g:RadioButton>
            <!--<g:RadioButton ui:field="manualSelection" debugId="manualSelection" name="strategyButton" value="false">
                <ui:msg description="Strategy manualSelection">MANUAL <br />SELECTION</ui:msg>
            </g:RadioButton>-->
    </g:FlowPanel>
</ui:UiBinder>