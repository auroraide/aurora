<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <g:VerticalPanel>
        <g:HorizontalPanel>
            <g:Label><ui:msg description="Delete LibraryItem Desc">Description:</ui:msg></g:Label>
            <g:TextArea ui:field="messageField"/>
        </g:HorizontalPanel>

        <g:HorizontalPanel>
            <g:Button ui:field="submitButton"><ui:msg description="Delete LibraryItem Submit">SUBMIT</ui:msg></g:Button>
            <g:Button ui:field="cancelButton"><ui:msg description="Delete LibraryItem Cancel">CANCEL</ui:msg></g:Button>
        </g:HorizontalPanel>

    </g:VerticalPanel>
</ui:UiBinder>